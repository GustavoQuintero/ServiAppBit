.barra-buscador
    width: 50%;
    margin-left: 29%;
.servicios
    background #2286a7 
    background-size cover
    margin-top -20px
    
 .lectura{
    display inline-block;
    float: left;
    margin-top: 34%;
    background: #1e90ff;
    border: 1px solid;
    padding: 17px 59px;
    margin-left: -257px;
    border-radius: 16px;
}

.lugar
    display block
    color black
    background white
    text-decoration none
    width 101%

.fechas
    display block
    width 101%

.valor
    display block
    width 101%

.publicar
    display block
    margin-left 3em;
    border-bottom 2px solid blue
    border-top 2px solid blue

.separados
    display inline-block
    margin-left 2em   
    padding 2% 2%
    margin-top 37px
    margin-bottom 37px
    border-radius 44px
    background brown
    border-radius 25px
    border solid 2px #BABABA
    box-shadow -1px -3px 5px 0px #BABABA
    padding 15px 15px 15px 19px
    min-height 220px
.foto
    border-radius 18px

.sisas
    padding 1em
    text-decoration none
    color white
    min-width 100%
    font-size 23px
.sisas-descrip
    width 94%
.Contenido-Servicio
    width 100%
.Fondo-Perfil
    background none
.Articulo-Servicio
    width 200%
    border-radius 15px !important
.Servicios-Usuarios
    display inline-block
.SS-Usuarios
   background #2286a7 !important
   margin-left 14%
.Usuarios-Servicios
    width 100%
    background #2286a7 !important
@media screen and (max-width: 1200px){
.busquedad {
          text-align: center;
          margin: 2em;
}
.resultado {
          text-decoration: none;
}
.lectura {
          display: inline-block;
          float: left;
          margin-top: 5em;
          background: blue;
          border: 1px solid;
          padding: 17px 59px;
          margin-left: 5px;
    }
.lugar{
        display: block;
        color: black;
        background: white;
        text-decoration: none;
    }
.fechas{
        display: block;
    }
.valor{
        display: block;
    }
.publicar {
        display: block;
        margin-left: 28%;
        border-bottom: 2px solid blue;
        border-top: 2px solid blue;
    }
.separados{
        display: inline-block;
        border: 1px solid red;
        margin-left: 3%;
        padding: 2% 3%;
        margin-top: 37px;
        margin-bottom: 37px;
    }
.encuentralo{
        text-align: center;
    }
.sigiente{
        margin-left: 20px;
        background: black;
        color: white !important;
        padding: 2px 4px;
        text-decoration: none !important;
    }
}
